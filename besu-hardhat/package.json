{
  "name": "my-hardhat",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc --noEmit",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.ts --network besu",
    "interact": "hardhat run scripts/interact.ts --network besu",
    "dashboard": "npx hardhat run dashboard/server.ts --network besu",
    "dashboard:kill": "lsof -ti:3001 | xargs kill -9 2>/dev/null || true",
    "dashboard:restart": "npm run dashboard:kill && sleep 1 && npm run dashboard",
    "test": "hardhat test",
    "test:dss": "hardhat test tests/dss-storage.test.ts --network besu",
    "test:network": "hardhat test tests/network.test.ts --network besu",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "hardhat clean"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^5.2.2",
    "@types/eslint__js": "^8.42.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.7.2",
    "chai": "^6.2.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "ethers": "^5.8.0",
    "hardhat": "^2.26.3",
    "jiti": "^2.6.1",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  }
}
